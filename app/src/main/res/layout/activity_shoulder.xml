<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="2dp"
    android:fillViewport="true"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="0dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Shoulder">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/ShoulderTitle"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <CheckBox
            android:id="@+id/checkBox7"
            android:layout_width="51dp"
            android:layout_height="48dp"
            android:layout_marginStart="328dp"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="@+id/clrbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/clrbutton"
            app:layout_constraintVertical_bias="0.507"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <CheckBox
            android:id="@+id/checkBox6"
            android:layout_width="51dp"
            android:layout_height="48dp"
            android:layout_marginStart="328dp"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="@+id/drrbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/drrbutton"
            app:layout_constraintVertical_bias="0.492" />

        <CheckBox
            android:id="@+id/checkBox5"
            android:layout_width="51dp"
            android:layout_height="48dp"
            android:layout_marginStart="328dp"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="@+id/dfrbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dfrbutton"
            app:layout_constraintVertical_bias="0.447" />

        <CheckBox
            android:id="@+id/checkBox4"
            android:layout_width="51dp"
            android:layout_height="48dp"
            android:layout_marginStart="328dp"
            android:elevation="3dp"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="@+id/dsrbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dsrbutton"
            app:layout_constraintVertical_bias="0.582" />

        <CheckBox
            android:id="@+id/checkBox2"
            android:layout_width="51dp"
            android:layout_height="48dp"
            android:layout_marginStart="328dp"
            android:layout_marginBottom="39dp"
            android:elevation="3dp"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="@+id/dspbutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dspbutton"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/dspbutton"
            android:layout_width="270dp"
            android:layout_height="115dp"
            android:layout_marginTop="16dp"
            android:checkable="false"
            android:elevation="1dp"
            android:text="@string/DumbbellShoulderPress"
            android:textAlignment="viewStart"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.645"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <Button
            android:id="@+id/dsrbutton"
            android:layout_width="270dp"
            android:layout_height="115dp"
            android:layout_marginTop="12dp"
            android:text="@string/DumbbellShoulderRaise"
            android:textAlignment="viewStart"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.644"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dspbutton" />

        <Button
            android:id="@+id/dfrbutton"
            android:layout_width="270dp"
            android:layout_height="115dp"
            android:layout_marginTop="8dp"
            android:text="@string/DumbbellFrontRaise"
            android:textAlignment="viewStart"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.644"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dsrbutton" />

        <Button
            android:id="@+id/drrbutton"
            android:layout_width="270dp"
            android:layout_height="115dp"
            android:layout_marginTop="16dp"
            android:text="@string/DumbbellRearDeltRaise"
            android:textAlignment="viewStart"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.644"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dfrbutton" />

        <Button
            android:id="@+id/clrbutton"
            android:layout_width="270dp"
            android:layout_height="115dp"
            android:layout_marginTop="8dp"
            android:text="@string/CableLateralRaise"
            android:textAlignment="viewStart"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.645"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/drrbutton" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
